  "page_parameters": {
    "projection": {
      "page_width": "1920px",
      "page_height": "1080px",
      "margin_top": "0px",
      "margin_bottom": "0px",
      "margin_left": "0px",
      "margin_right": "0px",
      "zoom": "1.0",
      "extra_options": ["--disable-smart-shrinking", "--enable-local-file-access"]
    },
    "a4_song": {
      "page_size": "A4",
      "orientation": "Portrait",
      "margin_top": "15",
      "margin_bottom": "15",
      "margin_left": "15",
      "margin_right": "15",
      "extra_options": ["--enable-local-file-access"]
    },
    "a4_toc": {
      "page_size": "A4",
      "orientation": "Portrait",
      "margin_top": "15",
      "margin_bottom": "15",
      "margin_left": "15",
      "margin_right": "15",
      "extra_options": ["--enable-local-file-access"]
    }
  },
  "output_formats": {
    "songbook_subdir_template": "{version}s_songbook"
  },
  "excel_column_mapping": {
    "Azon": "original_id",
    "Cím": "title",
    "Szerző": "author",
    "Dalszöveg": "lyrics",
    "Dalszöveg akkordokkal": "lyrics_with_chords",
    "Kategória": "category",
    "Youtube link": "youtube",
    "Érzékeny tartalom": "explicit_content",
    "Tartalomjegyzékből kimarad": "skip_toc",
    "Cím suffix": "title_suffix"
  },
  "lyrics": {
    "lines_thresholds": {
      "small": 40,
      "large": 25
    },
    "column_break_threshold": 60
  },

